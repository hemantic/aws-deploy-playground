[
  {
    "name": "${service_name}",
    "image": "${image_name}:latest",
    "cpu": 10,
    "memory": 256,
    "entryPoint": [
      "sh",
      "-c"
    ],
    "command": [
      "${command}"
    ],
    "secrets": [
      {
        "name": "SAMPLE_ENV_VAR",
        "valueFrom": "${sample_env_var}"
      }
    ],
    "environment": [
      {
        "name": "REDIS_URL",
        "value": "${redis_url}"
      }
    ],
    "portMappings": [
      {
        "containerPort": 0,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
          "awslogs-group": "${service_name}",
          "awslogs-region": "${aws_region}",
          "awslogs-stream-prefix": "${log_stream_prefix}"
      }
    }
  }
]