[
  {
    "name": "${service_name}",
    "image": "${image_name}:latest",
    "cpu": 10,
    "memory": 256,
    "entryPoint": [
      "/"
    ],
    "command": [
      "uwsgi --http :8000 --module srv.web:app --workers 1 --threads 1"
    ],
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ]
  }
]